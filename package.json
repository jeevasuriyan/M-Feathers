{
  "name": "feathers-work",
  "description": "exmaple",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run compile && npm run mocha",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "ts-node-dev --no-notify src/",
    "start": "npm run compile && node lib/",
    "mocha": "mocha --require ts-node/register --require source-map-support/register \"test/**/*.ts\" --recursive --exit",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.18",
    "@feathersjs/authentication-local": "^4.5.18",
    "@feathersjs/authentication-oauth": "^4.5.18",
    "@feathersjs/configuration": "^4.5.17",
    "@feathersjs/errors": "^4.5.17",
    "@feathersjs/express": "^4.5.18",
    "@feathersjs/feathers": "^4.5.17",
    "@feathersjs/socketio": "^4.5.18",
    "@feathersjs/transport-commons": "^5.0.11",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-mongoose": "^8.5.1",
    "helmet": "^5.1.1",
    "mongodb-core": "^3.2.7",
    "mongoose": "^6.12.2",
    "serve-favicon": "^2.5.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.4",
    "@types/cors": "^2.8.15",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/mocha": "^10.0.3",
    "@types/serve-favicon": "^2.5.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "axios": "^1.6.0",
    "eslint": "^8.52.0",
    "mocha": "^10.2.0",
    "shx": "^0.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
